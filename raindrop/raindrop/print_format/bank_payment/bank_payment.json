 "html": "\r\n       <div style=\"display:flex; flex-direction:column;  height:90vh;\">\r\n           <div>\r\n              <table border=\"2\">\r\n          <tr  style=\"font-weight:bold; padding:10px;\">\r\n              <!--<td rowspan=\"2\" colspan=\"1\"></td>-->\r\n            <td colspan=\"5\" ><div class=\"center\">{{doc.company}}</div><div  class=\"center\"> Tel NO:</div></td>\r\n            </tr>\r\n            <tr>\r\n            <td colspan=\"5\">\r\n                <div style=\"display:flex;\"><div>Voucher Number:</div><div>{{ doc.custom_document_number }}</div></div>\r\n                 <div style=\"display:flex;\">Date:<div>{{doc.posting_date}}</div></div>\r\n            </td>\r\n            </tr>\r\n          <tr class=\"h1\">\r\n             \r\n            <td colspan=\"5\" ><div style=\"text-align:center\"> Bank Payment (HO)</div></td>\r\n          </tr>\r\n          <tr >\r\n            <td colspan=\"2\">PARTICULARS</td>\r\n            <td>Narrations</td>\r\n            <td>Cheque</td>\r\n            <td >Amount</td>\r\n            \r\n          </tr>\r\n          \r\n       \r\n          <tr style= \"border:none\">\r\n              <td colspan=\"2\">{{ doc.paid_to }}</td>\r\n              <td> {{ doc.reference_no }}</td>\r\n              <td> {{doc.mode_of_payment}} </td>\r\n              <td> {{doc.paid_amount}}</td>\r\n               \r\n          </tr>\r\n       \r\n          <tr >\r\n              <td style= \"border:none\" ></td>\r\n              <td  style= \"border:none\" ></td>\r\n              <td style= \"border:1px solid black;\">Total</td>\r\n              <td  style= \"border:none\" ></td>\r\n              <td style= \"border:1px solid black;\">{{doc.paid_amount}}</td>\r\n          </tr>\r\n          <tr>\r\n              <td>Amount in words</td>\r\n              <td colspan=\"4\" >{{frappe.utils.money_in_words(doc.paid_amount)}}</td>\r\n            \r\n          </tr>\r\n          <!--<tr>-->\r\n          <!--   <td colspan=\"4\" >